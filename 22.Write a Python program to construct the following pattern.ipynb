    "rows = input(\"Enter max star to be display on single line: \")\n",
    "rows = int (rows)\n",
    "for i in range (1, rows+1):\n",
    "    for j in range(1, i + 1):\n",
    "        print(j, end=' ')\n",
    "    print(\"\\r\")\n",
    "for i in range (rows+1, 0, -1):\n",
    "    for j in range(1, i -1):\n",
    "        print(j, end=' ')\n",
    "    print(\"\\r\")"
